{
  "name": "song-lookup-service-deploy",
  "$schema": "../../node_modules/nx/schemas/project-schema.json",
  "sourceRoot": "apps/song-lookup-service-deploy/src",
  "projectType": "application",
  "targets": {
    "synth": {
      "executor": "nx:run-commands",
      "options": {
        "cwd": "apps/song-lookup-service-deploy",
        "command": "cdk synth --context env=dev"
      },
      "dependsOn": [{
        "projects": ["cdk-lib"],
        "target": "install-deps"
      }],
      "configurations": {
        "prod": {
          "command": "cdk synth --context env=prod"
        },
        "dev": {
          "command": "cdk synth --context env=dev"
        }
      }
    },
    "deploy": {
      "executor": "nx:run-commands",
      "options": {
        "cwd": "apps/song-lookup-service-deploy",
        "command": "cdk deploy --require-approval never --context env=dev"
      },
      "dependsOn": [
        {
          "projects": ["song-lookup-service-app"],
          "target": "install-deps"
        },
        "synth"
      ],
      "configurations": {
        "prod": {
          "command": "cdk deploy --require-approval never --context env=prod"
        },
        "dev": {
          "command": "cdk deploy --require-approval never --context env=dev"
        }
      }
    }
  },
  "tags": []
}
