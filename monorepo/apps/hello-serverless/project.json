{
    "root": "apps/hello-serverless",
    "projectType": "application",
    "sourceRoot": "apps/hello-serverless/src",
    "targets": {
      "build": {
        "executor": "nx:run-commands",
        "options": {
          "cwd": "apps/hello-serverless",
          "color": true,
          "command": "sls package"
        }
      },
      "deploy": {
        "executor": "nx:run-commands",
        "options": {
          "cwd": "apps/hello-serverless",
          "color": true,
          "command": "sls deploy --verbose"
        },
        "dependsOn": [
          {
            "target": "deploy",
            "projects": "dependencies"
          }
        ]
      }
    },
    "tags": ["service"],
    "implicitDependencies": []
  }